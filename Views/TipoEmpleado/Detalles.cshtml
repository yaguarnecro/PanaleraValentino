
@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles</h2>

<div>

    <form>

        <div class="form-group">
            <label for="id"> IdTipoEmpleado: </label>
            <label for="id" name="IdTipoEmpleado">@Model.IdTipoEmpleado </label>
            <input type="hidden" name="IdTipoEmpleado" value="@Model.IdTipoEmpleado" />
        </div>

        <div class="form-group">

            <label for="Descripcion"> Descripcion: </label>
            <label for="Descripcion" name="Descripcion">@Model.Descripcion </label>
            <input type="hidden" name="Descripcion" value="@Model.Descripcion" />
        </div>





    </form>

</div>

